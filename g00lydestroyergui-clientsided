-- Scripts:

local function AURF_fake_script() -- G00LYAUDIO1.LocalScript 
	local script = Instance.new('LocalScript', G00LYAUDIO1)

	-- LocalScript placed inside the TextButton
	
	-- Get the TextButton
	local textButton = script.Parent
	
	-- Create a new Sound object
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://131368101796681"
	sound.Parent = game.Workspace -- You can change the parent if needed
	
	-- Function to play the sound
	local function playSound()
		-- Play the sound only if it's not already playing
		if not sound.IsPlaying then
			sound:Play()
		end
	end
	
	-- Connect the function to the TextButton's MouseButton1Click event
	textButton.MouseButton1Click:Connect(playSound)
	
end
coroutine.wrap(AURF_fake_script)()
