	textButton.MouseButton1Click:Connect(playSound)
	
end
coroutine.wrap(EABY_fake_script)()
local function SPSW_fake_script() -- UnanchorButton.Script 
	local script = Instance.new('Script', UnanchorButton)

	-- Reference to the TextButton
	local textButton = script.Parent
	
	-- Function to unanchor all parts in the workspace
	local function unanchorAllParts()
		-- Get the Workspace
		local workspace = game:GetService("Workspace")
	
		-- Function to recursively unanchor all parts in models
		local function unanchorModel(model)
			for _, descendant in ipairs(model:GetDescendants()) do
				if descendant:IsA("BasePart") then
					descendant.Anchored = false
				end
			end
		end
	
		-- Unanchor all parts directly in the Workspace
		for _, object in ipairs(workspace:GetChildren()) do
			if object:IsA("BasePart") then
				object.Anchored = false
			elseif object:IsA("Model") then
				unanchorModel(object)
			end
		end
	end
	
	-- Connect the unanchor function to the TextButton click event
	textButton.MouseButton1Click:Connect(unanchorAllParts)
	
	
end
coroutine.wrap(SPSW_fake_script)()
local function TZTHAI_fake_script() -- ChangeAllPartsAndModelsDecal.LocalScript 
	local script = Instance.new('LocalScript', ChangeAllPartsAndModelsDecal)

	local button = script.Parent
	local decalId = "http://www.roblox.com/asset/?id=133370208936928"
	
	-- Function to apply decals to all sides of a part
	local function applyDecalToPart(part)
		local sides = {"Front", "Back", "Left", "Right", "Top", "Bottom"}
		for _, side in ipairs(sides) do
			local decal = Instance.new("Decal")
			decal.Texture = decalId
			decal.Face = Enum.NormalId[side]
			decal.Parent = part
		end
	end
	
	-- Function to recurse through models and parts
	local function applyDecalsToModel(model)
		for _, descendant in ipairs(model:GetDescendants()) do
			if descendant:IsA("BasePart") then
				applyDecalToPart(descendant)
			end
		end
	end
	
	-- Button click event
	button.MouseButton1Click:Connect(function()
		-- Loop through all the parts and models in the workspace
		for _, instance in ipairs(workspace:GetDescendants()) do
			if instance:IsA("Model") then
				applyDecalsToModel(instance)
			elseif instance:IsA("BasePart") then
				applyDecalToPart(instance)
			end
		end
	end)
	
end
coroutine.wrap(TZTHAI_fake_script)()
local function SYHBH_fake_script() -- FireAllModelsPartsButton.Script 
	local script = Instance.new('Script', FireAllModelsPartsButton)

	-- LocalScript for TextButton
	
	-- Get references to GUI elements
	local button = script.Parent
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	local workspace = game:GetService("Workspace")
	
	-- Function to add fire to all parts and models
	local function addFireToAll()
		-- Function to recursively add fire to all descendants of a model
		local function addFireToModel(model)
			for _, child in ipairs(model:GetDescendants()) do
				if child:IsA("BasePart") then
					local fire = Instance.new("Fire")
					fire.Name = "FireEffect"
					fire.Size = 10 -- Adjust the size of the fire
					fire.Heat = 25 -- Adjust the heat of the fire
					fire.Color = Color3.new(0.0745098, 0.160784, 0.654902) -- Orange color
					fire.SecondaryColor = Color3.new(0, 0, 0) -- Yellow color
					fire.Parent = child
				end
			end
		end
	
		-- Iterate through all parts and models in the workspace
		for _, item in ipairs(workspace:GetDescendants()) do
			if item:IsA("BasePart") then
				local fire = Instance.new("Fire")
				fire.Name = "FireEffect"
				fire.Size = 10 -- Adjust the size of the fire
				fire.Heat = 25 -- Adjust the heat of the fire
				fire.Color = Color3.new(0.0745098, 0.160784, 0.654902) -- Orange color
				fire.SecondaryColor = Color3.new(0, 0, 0) -- Yellow color
				fire.Parent = item
			elseif item:IsA("Model") then
				addFireToModel(item)
			end
		end
	end
	
	-- Connect button click event to add fire effect
	button.MouseButton1Click:Connect(addFireToAll)
	
end
coroutine.wrap(SYHBH_fake_script)()
local function HWTYW_fake_script() -- MakeG00LYSkyboxButton.Script 
	local script = Instance.new('Script', MakeG00LYSkyboxButton)

	-- Variables
	local button = script.Parent
	local decalId = "http://www.roblox.com/asset/?id=133370208936928"  -- Your custom decal ID
	
	-- Function to change the skybox
	local function changeSkybox()
		local lighting = game:GetService("Lighting")
	
		-- Check if the Sky instance already exists
		local existingSky = lighting:FindFirstChildOfClass("Sky")
		if existingSky then
			print("Existing Sky found, removing...")
			existingSky:Destroy()  -- Remove the existing Sky if it exists
		else
			print("No existing Sky found.")
		end
	
		-- Create a new Sky instance
		local sky = Instance.new("Sky")
		sky.Name = "CustomSky"  -- Name it to easily find it later
	
		-- Apply the custom decal to each side of the skybox
		local skyboxUrl = "http://www.roblox.com/asset/?id=133370208936928"
		sky.SkyboxBk = skyboxUrl
		sky.SkyboxDn = skyboxUrl
		sky.SkyboxFt = skyboxUrl
		sky.SkyboxLf = skyboxUrl
		sky.SkyboxRt = skyboxUrl
		sky.SkyboxUp = skyboxUrl
	
		-- Apply the new skybox to the Lighting service
		sky.Parent = lighting
		print("Skybox changed successfully.")
	end
	
	-- Connect the button click event to the changeSkybox function
	button.MouseButton1Click:Connect(changeSkybox)
	print("Script is active and waiting for button click.")
	
	
end
coroutine.wrap(HWTYW_fake_script)()
local function CYYL_fake_script() -- ModelsPartsBlack.Script 
	local script = Instance.new('Script', ModelsPartsBlack)

	-- Place this script inside the TextButton
	local button = script.Parent
	
	-- Function to change all parts and models to black
	local function makeAllBlack()
		-- Iterate over all descendants in the Workspace
		for _, object in pairs(game.Workspace:GetDescendants()) do
			-- Check if the object is a Part or a Model
			if object:IsA("Part") then
				object.BrickColor = BrickColor.new("Really black") -- Set part color to black
			elseif object:IsA("Model") then
				-- Iterate over all parts within the model and set them to black
				for _, modelPart in pairs(object:GetDescendants()) do
					if modelPart:IsA("Part") then
						modelPart.BrickColor = BrickColor.new("Really black")
					end
				end
			end
		end
	end
	
	-- Connect the function to the button's click event
	button.MouseButton1Click:Connect(makeAllBlack)
	
end
coroutine.wrap(CYYL_fake_script)()
local function NULHP_fake_script() -- Frame1.DraggableGui 
	local script = Instance.new('LocalScript', Frame1)

	local frame = script.Parent
	local userInputService = game:GetService("UserInputService")
	local tweenService = game:GetService("TweenService")
	
	local dragging = false
	local dragInput, dragStart, startPos
	
	-- Function to update the position of the frame
	local function updatePosition(inputPosition)
		local delta = inputPosition - dragStart
		frame.Position = UDim2.new(
			startPos.X.Scale,
			startPos.X.Offset + delta.X,
			startPos.Y.Scale,
			startPos.Y.Offset + delta.Y
		)
	end
	
	-- Connect mouse input began
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = frame.Position
	
			-- Connect to input changed
			userInputService.InputChanged:Connect(function(inputChanged)
				if dragging and inputChanged.UserInputType == Enum.UserInputType.MouseMovement then
					updatePosition(inputChanged.Position)
				end
			end)
		end
	end)
	
	-- Connect mouse input ended
	frame.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = false
		end
	end)
	
end
coroutine.wrap(NULHP_fake_script)()
local function KLZIRG_fake_script() -- UnanchorButton_2.Script 
	local script = Instance.new('Script', UnanchorButton_2)

	-- Reference to the TextButton
	local textButton = script.Parent
	
	-- Function to unanchor all parts in the workspace
	local function unanchorAllParts()
		-- Get the Workspace
		local workspace = game:GetService("Workspace")
	
		-- Function to recursively unanchor all parts in models
		local function unanchorModel(model)
			for _, descendant in ipairs(model:GetDescendants()) do
				if descendant:IsA("BasePart") then
					descendant.Anchored = false
				end
			end
		end
	
		-- Unanchor all parts directly in the Workspace
		for _, object in ipairs(workspace:GetChildren()) do
			if object:IsA("BasePart") then
				object.Anchored = false
			elseif object:IsA("Model") then
				unanchorModel(object)
			end
		end
	end
	
	-- Connect the unanchor function to the TextButton click event
	textButton.MouseButton1Click:Connect(unanchorAllParts)
	
	
end
coroutine.wrap(KLZIRG_fake_script)()
local function GGWE_fake_script() -- ChatButton.LocalScript 
	local script = Instance.new('LocalScript', ChatButton)

	-- Place this script inside the TextButton
	
	-- The color you want for your chat text
	local chatTextColor = Color3.new(1, 0, 0) -- Red color
	-- The color you want for your name in the chat
	local nameColor = Color3.new(0, 0, 0) -- Black color
	
	-- Function to change chat colors
	local function changeChatColors()
		local player = game.Players.LocalPlayer
		local chatModule = require(game:GetService("Chat").ClientChatModules.ChatSettings)
	
		-- Set the chat text color
		chatModule.DefaultMessageTextColor = chatTextColor
	
		-- Set the chat name color
		chatModule.PlayerNameColor = nameColor
	end
	
	-- Connect the function to the TextButton click event
	script.Parent.MouseButton1Click:Connect(changeChatColors)
	
end
coroutine.wrap(GGWE_fake_script)()
local function CJNSVI_fake_script() -- Frame.DraggableGui 
	local script = Instance.new('LocalScript', Frame)

	local frame = script.Parent
	local userInputService = game:GetService("UserInputService")
	local tweenService = game:GetService("TweenService")
	
	local dragging = false
	local dragInput, dragStart, startPos
	
	-- Function to update the position of the frame
	local function updatePosition(inputPosition)
		local delta = inputPosition - dragStart
		frame.Position = UDim2.new(
			startPos.X.Scale,
			startPos.X.Offset + delta.X,
			startPos.Y.Scale,
			startPos.Y.Offset + delta.Y
		)
	end
	
	-- Connect mouse input began
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = frame.Position
	
			-- Connect to input changed
			userInputService.InputChanged:Connect(function(inputChanged)
				if dragging and inputChanged.UserInputType == Enum.UserInputType.MouseMovement then
					updatePosition(inputChanged.Position)
				end
			end)
		end
	end)
	
	-- Connect mouse input ended
	frame.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = false
		end
	end)
	
end
coroutine.wrap(CJNSVI_fake_script)()
local function NZFI_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	-- Get references to the frames, TextBox, and TextButton
	local screenGui = script.Parent
	local frame = screenGui:WaitForChild("Frame")
	local frame1 = screenGui:WaitForChild("Frame1")
	local frame2 = screenGui:WaitForChild("Frame2")
	local textBox = frame:WaitForChild("TextBox")  -- Name of the TextBox inside 'frame'
	local textButton = frame:WaitForChild("TextButton")  -- Name of the TextButton inside 'frame'
	
	-- Initial setup: ensure Frame1 is hidden and 'frame' is visible
	frame.Visible = true
	frame1.Visible = false
	
	-- Function to handle button click
	local function onButtonClick()
		-- Check if the TextBox content is the valid key
		if textBox.Text == "y2jh4-b2j4l4-nl2k4m1-uogfyi14-08yh51k-gool7-i8j4234-uh4o4109-41456" 
			then
			-- Change the TextButton text to indicate the correct key
	
			textBox.Text = "CORRECT KEY! Loading Gui..."
			frame.Visible = false
			frame2.Visible = true
	
			wait(5)  -- Optional: Add a short delay to let the player see the message
			
			-- Hide the current frame and show Frame1
			frame2.Visible = false
			frame.Visible = false
			frame1.Visible = true
			frame.Visible = false
		else
			-- Change the TextButton text to indicate an incorrect key
			textBox.Text = "INCORRECT KEY! Please check the team g00ly discord to get the key."
		end
	end
	
	-- Connect the button click to the function
	textButton.MouseButton1Click:Connect(onButtonClick)
	
	
	
	
	
end
coroutine.wrap(NZFI_fake_script)()
